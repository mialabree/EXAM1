// Run test cases

#include <ionstream>
using namespace std;

int main() {
    int array[5] = {5, 2, 5, 2, 3};
    int x = 0;

    cout << "The original array = " << endl;

    for (int i = 0; i < 5; i++) {
        cout << array[i] << endl;
    }

    cout << endl:
    cout << "The original number in the Array = ";

    for (int i = 0; i < 5; i++) {
        x = x^array[i];
    }

    cout << x << endl;

}
